a,b = map(int, input().split())
#나누기(실수형)
print(a/b)
#몫(정수)
print(a//b)
#나머지값
print(a%b)

a = input()
#문자열 + 연결
print(a + "??!") 
#정수
print(int(a))

# \는 하나더입력 "는 ''로묶기
print("\\    /\\")
print(" )  ( ')")
print("(  /  )")
print(" \\(__)|")

print("|\\_/|")
print("|q p|   /}")
print('( 0 )"""'+"\\")
print('|"'+'^"'+"`    |")
print("||_/=\\\\__|")
